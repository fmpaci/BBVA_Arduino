void LeerReloj(){
  /*Variables para imprimir en el display*/
  bool Century=false;
  bool h12;
  bool PM;
  int aux=minute;
  /*Leer las variables del Reloj*/
  minute=Clock.getMinute();
  if(aux != minute && aux > minute){ //si ya es otra hora
    aux = hour;
    hour=Clock.getHour(h12, PM);
    if(aux != hour && aux > hour){ //si ya es otro mes
      date=Clock.getDate();
      month=Clock.getMonth(Century);
      year=Clock.getYear();
    }
  }
}


